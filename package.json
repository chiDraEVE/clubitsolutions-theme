{
  "name": "clubitsolutions-theme",
  "version": "1.0.0",
  "description": "my theme for my wordpress projects",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.config.prod.js",
    "build:custom": "wp-scripts build ./src/index.js ./src/customizer.js ./src/navigation.js --output-path=assets",
    "check-engines": "wp-scripts check-engines",
    "check-licenses": "wp-scripts check-licenses",
    "format": "wp-scripts format",
    "lint:css": "wp-scripts lint-style",
    "lint:js": "wp-scripts lint-js",
    "lint:md:docs": "wp-scripts lint-md-docs",
    "lint:md:js": "wp-scripts lint-md-js",
    "lint:pkg-json": "wp-scripts lint-pkg-json",
    "packages-update": "wp-scripts packages-update",
    "start": "webpack serve --config webpack.config.dev.js",
    "start:wp": "wp-scripts start",
    "test:e2e": "wp-scripts test-e2e",
    "test:unit": "wp-scripts test-unit-js",
    "watch": "node-sass sass/ -o ./ --source-map true --output-style expanded --indent-type tab --indent-width 1 -w",
    "compile:css": "node-sass sass/ -o ./ && stylelint '*.css' --fix || true && stylelint '*.css' --fix",
    "compile:rtl": "rtlcss style.css archive/style-rtl.css",
    "lint:scss": "wp-scripts lint-style 'sass/**/*.scss'",
    "bundle": "dir-archiver --src . --dest ../_s.zip --exclude .DS_Store .stylelintrc.json .eslintrc .git .gitattributes .github .gitignore README.md composer.json composer.lock node_modules vendor package-lock.json package.json .travis.yml phpcs.xml.dist sass style.css.map"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chiDraEVE/clubitsolutions-theme.git"
  },
  "keywords": [
    "WordPress",
    "Theme",
    "clubITsolutions"
  ],
  "author": "chiDraEVE",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chiDraEVE/clubitsolutions-theme/issues"
  },
  "homepage": "https://github.com/chiDraEVE/clubitsolutions-theme#readme",
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-react": "^7.16.5",
    "@wordpress/scripts": "^19.2.2",
    "autoprefixer": "^10.4.0",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.3.0",
    "normalize.css": "^8.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sass-loader": "^12.4.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0",
    "webpack-merge": "^5.8.0",
    "webpack-nano": "^1.1.1",
    "dir-archiver": "^1.1.1",
    "node-sass": "^4.14.1",
    "rtlcss": "^2.5.0"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "clean-webpack-plugin": "^4.0.0",
    "fs-extra": "^10.0.0",
    "mini-css-extract-plugin": "^2.4.5",
    "postcss-color-function": "^4.1.0",
    "postcss-hexrgba": "^2.0.1",
    "postcss-import": "^14.0.2",
    "postcss-mixins": "^9.0.1",
    "postcss-nested": "^5.0.6",
    "postcss-simple-vars": "^6.0.3",
    "webpack-manifest-plugin": "^4.0.2"
  }
}
